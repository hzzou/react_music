
@import"../../fontSize.styl"
.result
	ul
		margin 0
		list-style none
		padding 0 20px
		li
			padding 20px 0
			border-bottom 1PX solid #ccc
			a
				text-decoration none
				color #666
				display flex
				justify-content space-between
